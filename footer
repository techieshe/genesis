//* Change the footer text
add_filter('genesis_footer_creds_text', 'sp_footer_creds_filter');
function sp_footer_creds_filter( $creds ) {
$creds = '[footer_copyright] &hearts; <a href="http://georyl.com">GEORYL</a> &hearts; Designed by <a href="http://techieshe.com" title="Techie She">Techie She</a> on <a href="http://techieshe.com/genesis" title="Genesis Framework">Genesis Framework</a>';
return $creds;
}
